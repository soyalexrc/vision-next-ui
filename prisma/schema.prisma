generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model AppConfig {
  id          String @id @default(cuid())
  code        String
  valie       String
  description String
}

model TemporalId {
  id String @id @default(cuid())
}

model DeleteFileRequest {
  id       String @id @default(cuid())
  path     String
  username String
  userId   String
  type     String
}
